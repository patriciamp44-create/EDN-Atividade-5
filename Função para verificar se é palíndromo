def eh_palindromo(texto: str) -> str:
    texto_limpo = ''.join(c.lower() for c in texto if c.isalnum())  # remove espaços e pontuação
    if texto_limpo == texto_limpo[::-1]:
        return "Sim"
    else:
        return "Não"

# Exemplo de uso:
print(eh_palindromo("Ame a ema"))  # Saída: Sim
print(eh_palindromo("Python"))     # Saída: Não
